{
	"author": {
		"name": "Rick Osborne",
		"url": "https://rickosborne.org"
	},
	"description": "Typescript implementation of game logic for the \"Beyond Baker Street\" card game by Z-Man Games",
	"devDependencies": {
		"@istanbuljs/esm-loader-hook": "^0.1.2",
		"@istanbuljs/nyc-config-typescript": "^1.0.1",
		"@types/better-sqlite3": "^5.4.1",
		"@types/chai": "^4.2.17",
		"@types/mocha": "^8.2.2",
		"@types/node": "^15.0.1",
		"@types/seedrandom": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"@zoltu/typescript-transformer-append-js-extension": "^1.0.1",
		"better-sqlite3": "^7.2.0",
		"chai": "^4.3.4",
		"eslint": "^7.25.0",
		"eslint-plugin-json": "^2.1.2",
		"esm": "^3.2.25",
		"mocha": "^7.0.0-esm1",
		"nyc": "^15.1.0",
		"seedrandom": "^3.0.5",
		"ts-node": "^9.1.1",
		"ttypescript": "^1.5.12",
		"typescript": "^4.2.4"
	},
	"keywords": [],
	"license": "UNLICENSED",
	"main": "index.js",
	"name": "beyond-baker-street-typescript",
	"scripts": {
		"clean": "rm -R ./node_modules *.log */*.d.ts || true",
		"cleancompile": "rm -R *.log */*.d.ts ./.nyc_output ./coverage ./js || true",
		"compile": "ttsc -p tsconfig.json",
		"coverage2": "NODE_OPTIONS='--experimental-loader=@istanbuljs/esm-loader-hook --experimental-specifier-resolution=node' nyc mocha --config .mocharc.json ts/*.spec.ts",
		"precoverage": "npm run compile",
		"coverage3": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' node node_modules/.bin/nyc mocha --config .mocharc.json js/*.spec.js",
		"coverage4": "NODE_OPTIONS='--experimental-loader ts-node/esm --experimental-specifier-resolution=node --trace-warnings' nyc mocha --config .mocharc.json ts/*.spec.ts",
		"coverage": "nyc mocha --config .mocharc.json ts/*.spec.ts",
		"lint": "eslint . --fix --ext .ts,.json",
		"precompile": "npm run cleancompile",
		"prereinstall": "npm run clean",
		"recompileAndRun": "npm run compile ; npm run start",
		"reinstall": "npm install",
		"optimize": "ts-node ts/optimizeWeights.ts optimizeWithInspectors4",
		"profile": "node js/index.js",
		"start": "ts-node index.ts",
		"test": "mocha --config .mocharc.json ts/*.spec.ts"
	},
	"version": "1.0.0"
}
